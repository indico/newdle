{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@lingui/core": "^3.0.0-21",
    "@lingui/react": "^3.0.0-21",
    "classnames": "^2.2.6",
    "final-form": "^4.20.1",
    "flask-urls": "^0.2.0",
    "indico-sui-theme": "0.0.4",
    "lodash": "^4.17.20",
    "make-plural": "^6.2.2",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "prop-types": "^15.7.2",
    "rc-time-picker": "^3.7.3",
    "react": "^16.13.1",
    "react-dates": "^21.8.0",
    "react-dom": "^16.13.1",
    "react-final-form": "^6.5.1",
    "react-redux": "^7.2.1",
    "react-responsive": "^8.1.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "semantic-ui-react": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@jedmao/redux-mock-store": "^3.0.5",
    "@lingui/cli": "^3.0.0-21",
    "@lingui/macro": "^3.0.0-21",
    "babel-core": "^7.0.0-bridge.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "flask-urls.macro": "^0.1.0",
    "node-sass": "^7.0.0",
    "preval.macro": "^5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "add-locale": "lingui add-locale",
    "extract": "rm -f src/locales/en/messages.* && lingui extract --clean",
    "compile": "lingui compile"
  },
  "lingui": {
    "catalogs": [
      {
        "path": "<rootDir>/src/locales/{locale}/messages",
        "include": [
          "<rootDir>/src"
        ],
        "exclude": [
          "**/node_modules/**"
        ]
      }
    ],
    "format": "po",
    "sourceLocale": "en",
    "locales": [
      "en",
      "es"
    ],
    "compileNamespace": "es"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
