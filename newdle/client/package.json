{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@lingui/core": "^3.14.0",
    "@lingui/react": "^3.14.0",
    "classnames": "^2.3.1",
    "final-form": "^4.20.7",
    "flask-urls": "^0.2.0",
    "indico-sui-theme": "0.0.7",
    "lodash": "^4.17.21",
    "make-plural": "^7.1.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.34",
    "prop-types": "^15.8.1",
    "rc-time-picker": "^3.7.3",
    "react": "^16.13.1",
    "react-dates": "^21.8.0",
    "react-dom": "^16.13.1",
    "react-final-form": "^6.5.9",
    "react-redux": "^8.0.2",
    "react-responsive": "^9.0.0-beta.10",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.6",
    "semantic-ui-react": "^2.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@jedmao/redux-mock-store": "^3.0.5",
    "@lingui/cli": "^3.14.0",
    "@lingui/macro": "^3.14.0",
    "babel-core": "^7.0.0-bridge.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "flask-urls.macro": "^0.1.0",
    "sass": "^1.64.1",
    "preval.macro": "^5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "extract": "rm -f src/locales/en/messages.* && lingui extract --clean",
    "compile": "lingui compile"
  },
  "lingui": {
    "catalogs": [
      {
        "path": "<rootDir>/src/locales/{locale}/messages",
        "include": [
          "<rootDir>/src"
        ],
        "exclude": [
          "**/node_modules/**"
        ]
      }
    ],
    "format": "po",
    "formatOptions": {
      "origins": true,
      "lineNumbers": false
    },
    "sourceLocale": "en",
    "locales": [
      "de",
      "en",
      "eo",
      "es",
      "fr",
      "he",
      "id",
      "it",
      "nb_NO",
      "ta"
    ],
    "compileNamespace": "es"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
